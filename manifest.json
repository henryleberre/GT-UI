{
    "manifest_version": 3,

    "name": "GT UI",
    "version": "1.0",

    "icons": {
		"128": "icons/Icon-128.png",
		"64":  "icons/Icon-64.png",
		"48":  "icons/Icon-48.png",
		"32":  "icons/Icon-32.png",
		"16":  "icons/Icon-16.png"
	},

    "content_scripts": [
        {
            "matches": [
                "https://oscar.gatech.edu/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    
    "web_accessible_resources": [
        {
            "resources": [
                "page_template.html",
                "gt-logo.svg",
                "github-logo.svg"
            ],
            "matches": [
                "https://oscar.gatech.edu/*"
            ]
        }
    ]
}